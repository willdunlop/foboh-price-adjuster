    "dayjs": "^1.11.13",
    "decimal.js": "^10.4.3",
    "next": "15.1.4",
    "prisma": "^6.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redoc": "^2.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.18.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^4.18.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "postcss": "^8",
