{
  "name": "foboh-pricing-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev && npm run prisma:generate",
    "db:seed": "ts-node prisma/seed/index.ts",
