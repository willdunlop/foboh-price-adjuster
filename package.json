    "@tanstack/react-table": "^8.20.6",
    "classnames": "^2.5.1",
    "decimal.js": "^10.4.3",
    "next": "^15.1.4",
    "prisma": "^6.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.18.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^4.18.3",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.4",
    "postcss": "^8.4.49",
